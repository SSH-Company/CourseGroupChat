{
  "expo": {
    "name": "Cirkle",
    "slug": "cirkle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/iphoneX.png",
      "tabletImage": "./assets/ipad.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
